{
	"useTabs": true,
	"singleQuote": true,
	"semi": true,
	"proseWrap": "always",
	"trailingComma": "all",
	"printWidth": 120,
	"arrowParens": "always",
	"bracketSameLine": false,
	"plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-embed", "prettier-plugin-sql"],
	"tailwindFunctions": ["twJoin", "twMerge"],
	"embeddedSqlTags": ["SQL"],
	"language": "sqlite",
	"keywordCase": "upper"
}
